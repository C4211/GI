    <Storyboard x:Key="GI.Body.Button.Content.Hide">
        <DoubleAnimation Storyboard.TargetProperty="(TextElement.Foreground).(Brush.Opacity)" To="0" Duration="0:0:0.2"></DoubleAnimation>
    </Storyboard>
    <Storyboard x:Key="GI.Body.Button.Content.Show">
        <DoubleAnimation Storyboard.TargetProperty="(TextElement.Foreground).(Brush.Opacity)" To="1" Duration="0:0:0.2"></DoubleAnimation>
    </Storyboard>
    <Storyboard x:Key="GI.Body.Button.Remove">
        <DoubleAnimation Storyboard.TargetProperty="(UIElement.Opacity)" To="0" Duration="0:0:0.2">
            <DoubleAnimation.EasingFunction>
                <PowerEase EasingMode="EaseInOut"/>
            </DoubleAnimation.EasingFunction>
        </DoubleAnimation>
        <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)">
            <EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="0.8">
