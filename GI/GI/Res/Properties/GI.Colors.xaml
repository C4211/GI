<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="GI.Colors.Essential">#2C77DE</SolidColorBrush>
    <LinearGradientBrush x:Key="GI.Colors.Head.linear" EndPoint="0,1">
        <GradientStop Color="#FF4B87D8" Offset="0"/>
        <GradientStop Color="#FF4080D7" Offset="1"/>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="GI.Colors.Secondary">#5AC65C</SolidColorBrush>
    <SolidColorBrush x:Key="GI.Colors.Background">#F5FFFFFF</SolidColorBrush>
    <SolidColorBrush x:Key="GI.Colors.Font.Head">#EDEDED</SolidColorBrush>
    <SolidColorBrush x:Key="GI.Colors.Head.ico">#FFFFFF</SolidColorBrush>
    <SolidColorBrush x:Key="GI.Colors.Body.Tile">#333333</SolidColorBrush>
    <SolidColorBrush x:Key="GI.Colors.Body.Summary">#999999</SolidColorBrush>
    <Color x:Key="GI.Colors.Body.FunctionGroup.Default">#00CCCCCC</Color>
    <Color x:Key="GI.Colors.Body.FunctionGroup.Hover">#FFCCCCCC</Color>
    <Color x:Key="GI.Colors.Body.FunctionGroup.Press">#FF2C77DE</Color>
</ResourceDictionary>