<Application x:Class="GI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Res/Properties/GI.Details.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Res/Properties/GI.Colors.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Res/Properties/GI.Effects.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Res/Properties/GI.Fonts.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Res/Icons/GI.Window.Head.Logo.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Res/Icons/GI.Window.Head.Close.xaml"></ResourceDictionary>
                <ResourceDictionary Source="ViewModel/GI.Styles.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Res/Icons/GI.Window.Head.Min.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
